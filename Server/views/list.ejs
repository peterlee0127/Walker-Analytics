<div>
    <label>Search:</label>
    <input type="text" ng-model="search">
    <div>Number of mark:{{ MotionData.length }}</div>
</div>
<div class="table-responsive">
<table class="table table-hover">
  <thead>
       <tr>
        <th class="col-md-1">time</th>
        <th class="col-md-1">activity</th>
        <th class="col-md-1">Relative-Altitude</th>
        <th class="col-md-1">Map-Altitude</th>
        <th class="col-md-1">Accuracy</th>
        <th class="col-md-1">floorIsAscended</th>
        <th class="col-md-1">latitude緯度,longitude經度</th>
  </tr>
  </thead>
  <tbody>
      <tr ng-repeat="data in MotionData  | orderBy:'timestamp' | filter:search">
          <td ng-click="deleteData(data._id)">{{ data.timestamp * 1000 | date:'MM/dd HH:mm:ss' }}</td>
          <td>{{ data.activity }}</td>
          <td>{{ data.altitudeLog }}</td>
          <td>{{ data.altitude }}</td>
          <td>{{ data.horizontalAccuracy }}</td>
          <td>{{ data.floorIsAscended }}</td>
          <td>{{ data.latitude }}，{{ data.longitude }}</td>

      </tr>
  </tbody>
</table>
</div>
